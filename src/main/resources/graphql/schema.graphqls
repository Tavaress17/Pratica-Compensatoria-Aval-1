type Classroom {
    id: ID!
    description: String!
    block: String!
    floor: String!
    capacity: Int!
    observation: String!
}

type Query {
    classrooms: [Classroom!]!
    classroom(id: ID!): Classroom
}

type Mutation {
    createClassroom(description: String!, block: String!, floor: String!, capacity: Int!, observation: String!): Classroom!
}